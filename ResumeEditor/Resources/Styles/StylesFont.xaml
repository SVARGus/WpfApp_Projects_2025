<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="StyleResumeFontTextBlock" TargetType="TextBlock">
        <Setter Property="FontSize" Value="20"/>
        <Setter Property="FontFamily" Value="Franklin Gothic Medium"/>
        <Setter Property="FontWeight" Value="ExtraBold"/>
        <Setter Property="Background" Value="Gray"/>
        <Setter Property="Margin" Value="0,2,0,2"/>
    </Style>
    <Style x:Key="StyleResumeFontTextBox" TargetType="TextBox">
        <Setter Property="FontSize" Value="20"/>
        <Setter Property="FontFamily" Value="Franklin Gothic Medium"/>
        <Setter Property="FontWeight" Value="ExtraBold"/>
        <Setter Property="Margin" Value="0,1,0,1"/>
    </Style>
    <Style x:Key="StyleResumeFontDatePicker" TargetType="DatePicker">
        <Setter Property="FontSize" Value="20"/>
        <Setter Property="FontFamily" Value="Franklin Gothic Medium"/>
        <Setter Property="FontWeight" Value="ExtraBold"/>
        <Setter Property="Background" Value="Gray"/>
    </Style>
    <Style x:Key="StyleResumeStackPanelWhite" TargetType="StackPanel">
        <Setter Property="Background" Value="FloralWhite"/>
        <Setter Property="Orientation" Value="Horizontal"/>
        <Setter Property="Margin" Value="0, 10"/>
        <Setter Property="TextElement.FontSize" Value="24"/>
        <Setter Property="TextElement.FontWeight" Value="Heavy"/>
    </Style>
</ResourceDictionary>