<Window x:Class="WPF_Examen_21_03_2025.Views.ResumeDateEntryFinal"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_Examen_21_03_2025.Views"
        mc:Ignorable="d"
        Title="ResumeDateEntryFinal" MinHeight="600" MinWidth="800"
        MaxHeight="1200" MaxWidth="1800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <ScrollViewer Grid.Row="0" VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Disabled">
            <Grid x:Name="SelectForm"/>
        </ScrollViewer>
        <Button Grid.Row="1" Content="Назад" FontSize="25" Margin="10" Click="Button_ClickBack"/>
        <Button Grid.Row="2" Content="Новое резюме" FontSize="25" Margin="10" Click="Button_Click_NewResume"/>
        <Button Grid.Row="3" Content="Сохранить" FontSize="25" Margin="10" Click="Button_Click_SaveResume"/>
    </Grid>
</Window>
