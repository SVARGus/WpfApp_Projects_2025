<Window x:Class="WPF_Examen_21_03_2025.Views.ResumeDateEntryFour"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_Examen_21_03_2025.Views"
        mc:Ignorable="d"
        Title="ResumeDateEntryFour" Height="300" Width="500">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Viewbox Grid.ColumnSpan="2" Margin="20, 0">
            <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" Text="Расскажите О себе"
FontSize="30" FontWeight="Bold"/>
        </Viewbox>
        <TextBox Grid.ColumnSpan="2" Grid.Row="1" Margin="10" Text="{Binding AboutMe}" Height="150"/>
        <Button Grid.Column="0" Grid.Row="2" Content="Назад" FontSize="25" Margin="10" Click="Button_ClickBack"/>
        <Button Grid.Column="1" Grid.Row="2" Content="Далее" FontSize="25" Margin="10" Click="Button_ClickLater"/>
    </Grid>
</Window>
